<div class="container-fluid">
  <div class="row">
    <div class="col-lg-6">
      <h4>Projects</h4>
    </div>
  </div>
<br />
<div class="container">
  <div class="row">
    <div class="col-lg-12">
      <% @projects.each do |project| %>
        <div class="col-lg-3 bg-primary custom-style">  <!-- panel panel-primary -->
          <p><b>Name: </b><%= project.name %><p>
          <p><b>Category: </b><%= project.category %></p>
          <p><b>Framework: </b><%= project.framework %></p>
        </div>
      <% end %>
    </div>
  </div>
</div>